{
    "name": "art2pro/crmapp",
    "description": "Test web app based on book Web Application Development with Yii2",
    "type": "project",
    "license": "public",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Artem Drachkov",
            "email": "artem.prof@gmail.com"
        }
    ],
    "config": {
        "vendor-dir": "vendor/composer"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "codeception/codeception": "2.2.9",
        "fzaninotto/faker": "1.6.0"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
